<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.coach.CoachMapper">

	<select id="getById" parameterType="int" resultType="entity.Coach">
		select * from coach where cid=#{cid}
	</select>
	
	<select id="getAll" resultType="entity.Coach">
		select * from coach
	</select>

</mapper>