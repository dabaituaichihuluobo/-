<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.sales.SalesMapper">

	<select id="getAll" resultType="entity.Sales">
		select * from sales
	</select>
	
	<select id="getById" resultType="entity.Sales" parameterType="int">
		select * from sales where sid=#{sid}
	</select>
	
	
</mapper>